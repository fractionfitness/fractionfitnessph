{
  "name": "fractionfitnessph",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "seed:generate": "ts-node -P tsconfig-seed.json -r tsconfig-paths/register --transpileOnly prisma/seedInputs.ts",
    "db": "docker compose up -d",
    "db:down": "docker compose down",
    "db:reset": "prisma db push && prisma db seed",
    "lint": "next lint",
    "format": "prettier '**/*.{js,jsx,cjs,ts,tsx,css,scss,sass,md}' --cache --write",
    "prepare": "husky install",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node -P tsconfig-seed.json -r tsconfig-paths/register --transpileOnly prisma/seed.ts"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^4.15.0",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "bcrypt": "^5.1.0",
    "class-variance-authority": "^0.6.0",
    "client-only": "^0.0.1",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "next": "^13.4.3",
    "next-auth": "^4.22.1",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  }
}
